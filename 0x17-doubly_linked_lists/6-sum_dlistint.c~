#include "lists.h"
/**
 * sum_dlistint - insert node at  n index.
 *@head: head of the double linked list
 *@index: index of node to be extracted
 *@n: number to insert on new node
 *Return: node adress
 */
int sum_dlistint(dlistint_t *head)
{
	int add = 0;

	while (head)
	{
		add += head->n;
		head = head->next;
	}
	return (add);
}
